<body>
    <div class="createentry-body">
      <div class="para-1">
        <h1>Create New Entry</h1>
      </div>
        <div class="form">
    <form class="createentry-form" id="createentry-form" action="/entry" method="POST">
      <div class="form-group">
        <label for="title" class="form-label">Title</label><br/>
        <input type="text" id="title" name="title" required/>
      </div>
      <div class="form-group">
        <label for="content" class="form-label">Content</label><br/>
        <textarea type="text" form="createentry-form" id="content" name="content" rows="10" cols="70" required></textarea>
      </div>
      <input type="submit" value="Create Entry"/>
    </form>
    </div>
  </body>
